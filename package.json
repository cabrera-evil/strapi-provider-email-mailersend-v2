{
  "name": "strapi-provider-email-mailersend-v2",
  "version": "0.0.6",
  "description": "Custom Strapi Provider Plugin For Email Using MailerSend",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "strapi",
    "email",
    "provider",
    "mailsersend"
  ],
  "homepage": "https://github.com/cabrera-evil/strapi-provider-email-mailersend-v2",
  "bugs": {
    "url": "https://github.com/cabrera-evil/strapi-provider-email-mailersend-v2/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cabrera-evil/strapi-provider-email-mailersend-v2"
  },
  "license": "MIT",
  "author": {
    "name": "cabrera-evil",
    "email": "douglascabrera.dev@gmail.com"
  },
  "maintainers": [
    {
      "name": "cabrera-evil",
      "email": "douglascabrera.dev@gmail.com"
    }
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --project tsconfig.build.json && tsc-alias",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index",
    "start": "ts-node -r tsconfig-paths/register dist/index",
    "lint": "eslint --fix .",
    "lint:check": "eslint .",
    "lint:fix": "pnpm lint && pnpm format",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "pnpm lint:check",
      "pnpm format:check"
    ]
  },
  "dependencies": {
    "mailersend": "^2.3.0",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.18.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.19.1"
  },
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  }
}
